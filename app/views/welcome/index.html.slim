    header.section-header
      section.header-main.border-bottom
        .container
          .row.align-items-center
            .col-lg-2.col-4
              a.brand-wrap href="http://bootstrap-ecommerce.com"
                img.logo src="assets/logo.png" /
              /! brand-wrap.//
            .col-lg-6.col-sm-12
              form.search action="#"
