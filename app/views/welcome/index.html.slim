                      i.fa.fa-search
                      | Search
              /! search-wrap .end//
            /! col.//
            .col-lg-4.col-sm-6.col-12
              .widgets-wrap.float-md-right
                .widget-header.mr-3
                  a.icon.icon-sm.rounded-circle.border href="#"
                    i.fa.fa-shopping-cart
                  span.badge.badge-pill.badge-danger.notify 0
                .widget-header.icontext
                  a.icon.icon-sm.rounded-circle.border href="#"
                    i.fa.fa-user
                  .text
                    span.text-muted Welcome!
                    div
                      a href="#"  Sign in
                      |
                      a href="#"  Register
              /! widgets-wrap.//
            /! col.//
          /! row.//
        /! container.//
      /! header-main .//
